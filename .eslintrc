{
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "react/jsx-filename-extension": "off",
    "react/no-danger": "off",
    "no-underscore-dangle": "off",
    "react/jsx-indent": "off",
    "global-require": "off"
  },
  "env": {
    "jest": true,
    "browser": true,
    "node": true
  },
  "parser": "babel-eslint"
}